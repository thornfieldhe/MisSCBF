
<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">实际支出与预算支出对比明细表</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                            <tr role="row">
                                <th style="width: 200px;" rowspan="3">
                                    项目名称(品名)
                                </th>
                                <th style="width: 200px;" rowspan="3">
                                    计量单位
                                </th>
                                <th style="width: 200px;" rowspan="3">
                                    数量
                                </th>
                                <th style="width: 200px;" rowspan="3">
                                    单价或经费标准或支出限额
                                </th>
                                <th style="width: 200px;" rowspan="3">
                                    合计(万元)
                                </th>
                                <th style="width: 1000px;" colspan="5">
                                    经费数额
                                </th>                                    
                                <th style="width: 200px;" rowspan="3">
                                    实际支出
                                </th>                                    
                                <th style="width: 200px;" rowspan="3">
                                    预算结余
                                </th>
                                <th style="width: 60px;" rowspan="3">
                                    操作
                                </th>
                            </tr>
                            <tr>
                                <th style="width: 200px;" rowspan="2">
                                    合计
                                </th>
                                <th style="width: 600px;" colspan="3">
                                    对下供应
                                </th>

                                <th style="width: 200px;" rowspan="2" >
                                    直接支出
                                </th>
                            </tr>
                            <tr>
                                <th style="width: 200px;">
                                    小计
                                </th>
                                <th style="width: 200px;">
                                    标准经费
                                </th>
                                <th style="width: 200px;">
                                    项目经费
                                </th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr v-for="item in list">
                                <td>{{item.name}}</td>
                                <td>{{item.unit}}</td>
                                <td>{{item.amount}}</td>
                                <td>{{item.price}}</td>
                                <td>{{item.total1}}</td>
                                <td>{{item.total2}}</td>
                                <td>{{item.total3}}</td>
                                <td>{{item.column1}}</td>
                                <td>{{item.column2}}</td>
                                <td>{{item.column3}}</td>
                                <td>{{item.total4}}</td>
                                <td><span class="red" v-if="item.surplus<0">{{item.surplus}}</span><span v-else>{{item.surplus}}</span> </td>
                                <td><button type="button" class="btn btn-default" data-dismiss="modal" v-on:click="showDetails(item.id)"> <i class="fa    fa-file-text-o"></i> 明细</button>
                                </td>

                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<form-body></form-body>
<template id="actualOutlayFormBody">
    <div class="bootbox modal fade modal-primary in" id="showOutlayModal">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">
                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                    <h4 class="modal-title">明细</h4>
                    <div v-model="title"></div>
                </div>
                <div class="modal-body">
                    <table class="table table-striped table-hover table-bordered ">
                        <thead>
                            <tr>
                                <th style="width: 200px;">
                                    凭证号
                                </th>
                                <th style="width: 200px;">
                                    时间
                                </th>
                                <th style="width: 200px;">
                                    金额
                                </th>
                                <th style="width: 400px;">
                                    摘要
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                        <tr v-for="row in list2">
                            <td>{{row.voucherNo}}</td>
                            <td>{{row.date}}</td>
                            <td> {{row.amount}}  </td>
                            <td>{{row.note}}</td>
                        </tr>
                        </tbody>
                    </table>
                </div>
                <div class="modal-footer">
                    <button type="button" class="btn btn-default" data-dismiss="modal"><i class='fa   fa-mail-reply'></i>关闭</button>
                </div>
            </div>
        </div>
    </div>
</template>
<script src="~/assets/js/liteuploader.js"></script>
<script src="~/Scripts/outlay.js"></script>







