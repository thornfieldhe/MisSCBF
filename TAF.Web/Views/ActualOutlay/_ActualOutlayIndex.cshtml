<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">实际预算支出</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                    <input type="file" name="fileUpload" id="fileUpload" class="fileUpload" title="上传实际预算支出" />
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-xs-5 col-md-5">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-danger">
                <span class="widget-caption">年初预算支出表</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                                <tr role="row">
                                    <th style="width: 60px;" rowspan="3">
                                        操作
                                    </th>
                                    <th style="width: 200px;" rowspan="3">
                                        项目名称(品名)
                                    </th>
                                    <th style="width: 200px;" rowspan="3">
                                        数量
                                    </th>
                                    <th style="width: 200px;" rowspan="3">
                                        计量单位
                                    </th>
                                    <th style="width: 200px;" rowspan="3">
                                        单价或经费标准或支出限额
                                    </th>
                                    <th style="width: 200px;" rowspan="3">
                                        合计(万元)
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr v-for="item2 in list2">
                                    <td>
                                        <div class="checkbox">
                                            <label for="selectItem_{{item2.id}}">
                                                <input type="radio" v-model="selectItem" value="{{item2.id}}" name="selectItem" id="selectItem_{{item2.id}}">
                                                <span class=" text"></span>
                                            </label>
                                        </div>
                                    </td>
                                    <td>{{item2.name}}</td>
                                    <td>{{item2.unit}}</td>
                                    <td>{{item2.amount}}</td>
                                    <td>{{item2.price}}</td>
                                    <td>{{item2.total}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="col-xs-7 col-md-7">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">年度实际支出表</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                    <div class="row">
                        <div class="col-md-12">
                            <button type="button" class="btn btn-primary" v-on:click="update()" v-show="!editModel"><i class='fa    fa-floppy-o'></i>提交 </button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                                <tr role="row">
                                    <th style="width: 60px;">
                                        操作
                                    </th>
                                    <th style="width: 200px;">
                                        凭证号
                                    </th>
                                    <th style="width: 200px;">
                                        时间
                                    </th>
                                    <th style="width: 200px;">
                                        金额
                                    </th>
                                    <th style="width: 400px;">
                                        摘要
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr v-for="item in list">
                                    <td>
                                        <div class="checkbox">
                                            <label for="selectItems_{{item.id}}">
                                                <input type="checkbox" v-model="selectItems" value="{{item.id}}" name="selectItems" id="selectItems_{{item.id}}">
                                                <span class=" text"></span>
                                            </label>
                                        </div>
                                    </td>
                                    <td>{{item.voucherNo}}</td>
                                    <td>{{item.date}}</td>
                                    <td>{{item.amount}}</td>
                                    <td>{{item.note}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/assets/js/liteuploader.js"></script>
<script src="~/Scripts/actualOutlay.js"></script>







