@using SCBF.BaseInfo.Dto
<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">实物油料库</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                    <add-button title="新增"></add-button>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                                <tr role="row">
                                    <th style="width: 200px;">
                                        油料代管单位
                                    </th>        
                                    <th style="width: 100px;">
                                        油料标号
                                    </th>       
                                    <th style="width: 100px;">
                                        库存量
                                    </th>        
                                    <th style="width: 200px;">
                                        操作
                                    </th>
                                </tr>
                                <tr role="row">
                                    <th>
                                    <select id="searchStoreId" style="width: 100%" v-model="queryEntity.storeId">
                                        <option value="">请选择...</option>
                                        @{
                                            foreach (var item in (List<SCBF.BaseInfo.Dto.SysDictionaryListDto>)ViewData["list1"])
                                            {
                                                <option value="@item.Id">@item.Value</option>
                                            }
                                        }
                                    </select>
                                    </th>        
                                    <th>
                                    </th>        
                                    <th>
                                    </th>        
                                    <th><search-command></search-command></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr v-for="item in list">
                                    <td>{{item.storeName}}</td>
                                    <td>{{item.octaneRatingName}}</td>
                                    <td>{{item.amount}}</td>
                                    <td>
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot is="table-foot" colspan="4"></tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<dialog-delete></dialog-delete>
<form-edit></form-edit>
<template id="octaneStoreFormBody">
    <div id="form">
        <validator name="v">
            <form class="form-horizontal " name="form" novalidate>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">油料代管单位<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <select id="searchEditStoreId" style="width: 100%" v-model="item.storeId" name="storeid" >
                                    <option value="" selected="selected">请选择...</option>
                                    @{
                                        foreach (var item in (List<SysDictionaryListDto>)ViewData["list1"])
                                        {
                                            <option value="@item.Id">@item.Value</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                    </div>
                </div> 
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">油料标号<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <select id="searchEditOctaneRatingId" style="width: 100%" v-model="item.octaneRatingId" name="octaneratingid" >
                                    <option value="" selected="selected">请选择...</option>
                                    @{
                                        foreach (var item in (List<SysDictionaryListDto>)ViewData["list2"])
                                        {
                                            <option value="@item.Id">@item.Value</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                    </div>
                </div> 
            </form>
        </validator>
    </div>
</template>
<script src="~/Views/OctaneStore/octaneStore.js"></script>



