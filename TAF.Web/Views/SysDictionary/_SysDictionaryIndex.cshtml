@model List<string>
<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">系统配置</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                    <add-button title="新增"></add-button>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                                <tr role="row">
                                    <th style="width: 200px;">
                                        分类
                                    </th> 
                                    <th style="width: 200px;">
                                        值
                                    </th>  
                                    <th style="width: 200px;">
                                        值2
                                    </th>        
                                    <th style="width: 200px;">
                                        值3
                                    </th>        
                                    <th style="width: 200px;">
                                        值4
                                    </th>        
                                    <th style="width: 200px;">
                                        值5
                                    </th>
                                    <th style="width: 200px;">
                                        备注
                                    </th>  
                                    <th style="width: 200px;">
                                        操作
                                    </th>
                                </tr>
                            <tr role="row">
                                <th>
                                    <select id="searchCategory" style="width: 100%" v-model="queryEntity.category">
                                        <option value="">请选择...</option>
                                        @{
                                            foreach (var item in Model)
                                            {
                                                <option value="@item">@item</option>
                                            }
                                        }
                                    </select>
                                </th>   
                                <th>
                                    <input type="text" class="form-control" placeholder="值" v-model="queryEntity.value"/>
                                </th>
                                <th>
                                    <input type="text" class="form-control" placeholder="值2" v-model="queryEntity.value2"/>
                                </th>
                                <th>
                                    <input type="text" class="form-control" placeholder="值3" v-model="queryEntity.value3"/>
                                </th>
                                <th>
                                    <input type="text" class="form-control" placeholder="值4" v-model="queryEntity.value4"/>
                                </th>
                                <th>
                                    <input type="text" class="form-control" placeholder="值5" v-model="queryEntity.value5"/>
                                </th>
                                <th>
                                    <input type="text" class="form-control" placeholder="备注" v-model="queryEntity.note"/>
                                </th>
                                <th><search-command></search-command></th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr v-for="item in list">
                                <td>{{item.category}}</td>
                                <td>{{item.value}}</td>
                                <td>{{item.value2}}</td>
                                <td>{{item.value3}}</td>
                                <td>{{item.value4}}</td>
                                <td>{{item.value5}}</td>
                                <td>{{item.note}}</td>
                                <td>
                                    <row-command :id="item.id" title="编辑" :name="item.value"></row-command>
                                </td>
                            </tr>
                            </tbody>
                            <tfoot is="table-foot" colspan="8"></tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<dialog-delete></dialog-delete>
<form-edit></form-edit>
<template id="sysDictionaryFormBody">
    <div id="form">
        <validator name="v">
            <form class="form-horizontal " name="form" novalidate>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">Category<span class="red">*</span></label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <select id="category" style="width: 100%" v-model="queryEntity.category" v-validate:category="{required:true}">
                                        <option value="">请选择...</option>
                                        @{
                                            foreach (var item in Model)
                                            {
                                                <option value="@item">@item</option>
                                            }
                                        }
                                    </select>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">Value<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <input type="text" class="form-control " placeholder="必填" name="value" v-model="item.value"  v-validate:value="{required:true}"/>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">Value2</label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <input type="text" class="form-control "  name="value2" v-model="item.value2"  />
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">Value3</label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <input type="text" class="form-control "  name="value3" v-model="item.value3"  />
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">Value4</label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <input type="text" class="form-control "  name="value4" v-model="item.value4"  />
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">Value5</label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <input type="text" class="form-control "  name="value5" v-model="item.value5"  />
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">Note</label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <textarea rows="5" class="form-control " name="note"  v-model="item.note"></textarea>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </validator>
    </div>
</template>

<script src="~/Views/SysDictionary/sysDictionary.js"></script>


