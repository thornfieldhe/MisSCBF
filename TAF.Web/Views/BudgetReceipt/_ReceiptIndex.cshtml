<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">预算</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="pre-scrollable">
                            <table class="table table-striped table-hover table-bordered " style="width: 3000px">
                                <thead>
                                    <tr role="row">
                                        <th style="width: 100px;" rowspan="4">
                                            科目
                                        </th>
                                        <th style="width: 200px;" rowspan="4">
                                            科目名称
                                        </th>
                                        <th style="width: 1800px;" colspan="18">
                                            预算收入(万元)
                                        </th>
                                        <th style="width: 700px;" colspan="7">
                                            实际收入(万元)
                                        </th>
                                        <th style="width: 100px;" rowspan="4">
                                            收入完成率%
                                        </th>
                                        <th style="width: 100px;" rowspan="4">
                                            操作
                                        </th>
                                    </tr>
                                    <tr role="row">
                                        <th style="width: 100px;" rowspan="3">
                                            合计
                                        </th>
                                        <th style="width: 700px;" colspan="7">
                                            年初预算收入
                                        </th>
                                        <th style="width: 700px;" colspan="7">
                                            预算调整
                                        </th>
                                        <th style="width: 300px;" colspan="3">
                                            调整后上级增加
                                        </th>
                                        <th style="width: 100px;" rowspan="3">
                                            合计
                                        </th>
                                        <th style="width: 600px;" colspan="6">
                                            预算调整
                                        </th>
                                    </tr>
                                    <tr role="row">
                                        <th style="width: 100px;" rowspan="2">
                                            小计
                                        </th>
                                        <th style="width: 100px;" colspan="3">
                                            向上请领
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            留用经费弥补
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            上年结转
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            当年预算外收入
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            小计
                                        </th>
                                        <th style="width: 300px;" colspan="3">
                                            向上请领
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            留用经费弥补
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            上年结转
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            当年预算外收入
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            小计
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            标准经费
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            项目经费
                                        </th>
                                        <th style="width: 300px;" colspan="3">
                                            向上请领
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            留用经费弥补
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            上年结转
                                        </th>
                                        <th style="width: 100px;" rowspan="2">
                                            当年实际预算外收入
                                        </th>
                                    </tr>
                                    <tr role="row">
                                        <th style="width: 100px;">小计</th>
                                        <th style="width: 100px;">标准经费</th>
                                        <th style="width: 100px;">项目经费</th>
                                        <th style="width: 100px;">小计</th>
                                        <th style="width: 100px;">标准经费</th>
                                        <th style="width: 100px;">项目经费</th>
                                        <th style="width: 100px;">小计</th>
                                        <th style="width: 100px;">标准经费</th>
                                        <th style="width: 100px;">项目经费</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr v-for="item in list">
                                        <td>{{item.code}}</td>
                                        <td>{{item.name}}</td>
                                        <td>{{item.total2}}</td>
                                        <td>{{item.total3}}</td>
                                        <td>{{item.total4}}</td>
                                        <td>{{item.total5}}</td>
                                        <td>{{item.total6}}</td>
                                        <td>{{item.total7}}</td>
                                        <td>{{item.total8}}</td>
                                        <td>{{item.total9}}</td>
                                        <td>{{item.total10}}</td>
                                        <td>{{item.total11}}</td>
                                        <td>{{item.total12}}</td>
                                        <td>{{item.total13}}</td>
                                        <td>{{item.total14}}</td>
                                        <td>{{item.total15}}</td>
                                        <td>{{item.total16}}</td>
                                        <td>{{item.total17}}</td>
                                        <td>{{item.total18}}</td>
                                        <td>{{item.total19}}</td>
                                        <td>{{item.total20}}</td>
                                        <td>{{item.total21}}</td>
                                        <td>{{item.total22}}</td>
                                        <td>{{item.total23}}</td>
                                        <td>{{item.total24}}</td>
                                        <td>{{item.total25}}</td>
                                        <td>{{item.total26}}</td>
                                        <td>{{item.total27}}%</td>
                                        <td>
                                            <button type="button" class="btn   edit" v-on:click="editItem(item.code,'当年实际预算外收入') "><i class="fa fa-edit"></i> 编辑</button>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<dialog-delete></dialog-delete>
<form-edit></form-edit>
<template id="receiptFormBody">
    <div id="form">
        <validator name="v">
            <form class="form-horizontal " name="form" novalidate>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">科目<span class="red">*</span></label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <input type="text" class="form-control "  name="code" v-model="item.code" readonly="readonly"/>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">科目名称</label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <input type="text" class="form-control " name="name" v-model="item.name" readonly="readonly" />
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
               <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">实际预算外收入<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <input type="text" class="form-control  " placeholder="必填" id="amount" name="amount" v-model="item.amount" v-validate:required="{int:true}"/>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </validator>
    </div>
</template>
<script src="~/Scripts/receipt.js"></script>



