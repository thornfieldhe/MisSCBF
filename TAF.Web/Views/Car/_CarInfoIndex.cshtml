<dialog-delete></dialog-delete>
<form-edit></form-edit>
<template id="carInfoFormBody">
    <div id="form">
        <validator name="v">
            <form class="form-horizontal " name="form" novalidate>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group">
                            <label class="col-sm-2 control-label no-padding-right">{{item.title}}<span class="red">*</span></label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <div v-if="item.category!='Car_OilWearSummary'&& item.category!='Car_OilWearWinter'">
                                    <input type="text" class="form-control " placeholder="必填" name="value" v-model="item.value" v-validate:value="{required:true}"/>
                                    </div>
                                    <div v-else>
                                    <input type="number" class="form-control " placeholder="必填" name="value" v-model="item.value" v-validate:value="{required:true}"/>
                                    </div>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </validator>
    </div>
</template>
<script src="~/Scripts/carInfoIndex.js"></script>




