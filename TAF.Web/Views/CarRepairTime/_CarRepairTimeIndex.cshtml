<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">车辆维修耗时管理</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                </div>
                <div class="row">
                    <div class="col-md-12">
                    <div class="table-scrollable">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                                <tr role="row">
                                    <th style="width: 200px;">
                                        维修厂
                                    </th>        
                                    <th style="width: 100px;">
                                        返修率
                                    </th>        
                                    <th style="width: 100px;">
                                        维修效率
                                    </th>      
                                    <th style="width: 100px;">
                                        操作
                                    </th>        
                                </tr>
                                <tr role="row">
                                    <th >
                                        <div class="col-sm-6">
                                            <input type="text" class="form-control" placeholder="起" v-model="queryEntity.dateFrom" id="datePickerDateFrom" readonly="readonly" />
                                        </div>
                                        <div class="col-sm-6">
                                            <input type="text" class="form-control" placeholder="止" v-model="queryEntity.dateTo" id="datePickerDateTo" readonly="readonly" />
                                        </div>
                                    </th>
                                    <th></th>
                                    <th></th>
                                    <th>
                                        <button type="button" class="btn edit" v-on:click="query(0)"><i class="fa fa-search"></i> 搜索</button>
                                        <button type="button" class="btn edit" v-on:click="resetSearch()"><i class="fa fa-times"></i> 重置</button>
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr v-for="item in list">
                                    <td>  {{item.serviceDepot}}</td>
                                    <td><div v-show ="item.value1>5"><span class="red"> {{item.value1}}%</span></div> <div v-show ="item.value1<5"> {{item.value1}}%</div></td>
                                    <td><div v-show ="item.value2<0"><span class="red"> 效率低下</span></div><div v-show="item.value2>=0"> 效率正常</div></td>
                                    <td></td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                     </div>
                </div>
            </div>
        </div>
    </div>
</div>


<script src="~/Views/CarRepairTime/carRepairTime.js"></script>



