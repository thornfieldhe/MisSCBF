
<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">物资报表</span>
            </div>
            <div class="widget-body">
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                                <tr role="row">
                                    <th style="width: 400px;">
                                        出入库时间
                                    </th>
                                    <th style="width: 200px;">
                                        出入库单号
                                    </th>
                                    <th style="width: 200px;">
                                        编码
                                    </th>
                                    <th style="width: 200px;">
                                        品名
                                    </th>
                                    <th style="width: 200px;">
                                        规格型号
                                    </th>
                                    <th style="width: 100px;">
                                        单位
                                    </th>
                                    <th style="width: 100px;">
                                        单价
                                    </th>
                                    <th style="width: 100px;">
                                        数量
                                    </th>
                                    <th style="width: 100px;">
                                        小计
                                    </th>
                                    <th style="width: 200px;">
                                        操作
                                    </th>
                                </tr>
                                <tr>
                                    <th>
                                        <div class="row">
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control" placeholder="起" v-model="queryEntity.dateFrom" id="dateFrom" readonly="readonly" />
                                            </div>
                                            <div class="col-sm-6">
                                                <input type="text" class="form-control" placeholder="止" v-model="queryEntity.dateTo" id="dateTo" readonly="readonly" />
                                            </div>
                                        </div>
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="出入库单号" v-model="queryEntity.code" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="编码" v-model="queryEntity.productCode" />
                                    </th>
                                    <th>
                                        <input type="text" class="form-control" placeholder="品名" v-model="queryEntity.name" />
                                    </th>
                                    <th>
                                    </th>
                                    <th>
                                    </th>
                                    <th>
                                    </th>
                                    <th>
                                    </th>
                                    <th>
                                    </th>
                                    <th><search-command></search-command></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr v-for="item in list">
                                    <td>{{item.date}}</td>
                                    <td>{{item.code}}</td>
                                    <td>{{item.productCode}}</td>
                                    <td>{{item.productName}}</td>
                                    <td>{{item.specifications}}</td>
                                    <td>{{item.unit}}</td>
                                    <td>{{item.price}}</td>
                                    <td>{{item.amount}}</td>
                                    <td><div v-if="item.category==2"><span class="red">{{item.total}}</span></div><div v-else>{{item.total}}</div></td>
                                    <td></td>
                                </tr>
                            </tbody>
                            <tfoot is="table-foot" colspan="10"></tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/Views/HisStock/queryStock.js"></script>



