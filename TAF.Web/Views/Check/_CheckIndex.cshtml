@model List<TAF.Utility.KeyValue<string, Guid>>
<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">盘点</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                    <add-button title="新增"></add-button>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                                <tr role="row">
                                    <th style="width: 200px;">
                                        ProductId
                                    </th>        
                                    <th style="width: 200px;">
                                        StockAmount
                                    </th>        
                                    <th style="width: 200px;">
                                        Amount
                                    </th>        
                                    <th style="width: 200px;">
                                        ChangedAmount
                                    </th>        
                                    <th style="width: 200px;">
                                        Reason
                                    </th>        
                                    <th style="width: 200px;">
                                        Price
                                    </th>        
                                    <th style="width: 200px;">
                                        StorageId
                                    </th>        
                                    <th style="width: 200px;">
                                        操作
                                    </th>
                                </tr>
                                <tr role="row">
                                    <th>
                                    <select id="searchProductName" style="width: 100%" v-model="queryEntity.productId">
                                        <option value="">请选择...</option>
                                        @{
                                            foreach (var item in Model)
                                            {
                                                <option value="@item.Value">@item.Key</option>
                                            }
                                        }
                                    </select>
                                    </th>        
                                    <th>
                                    </th>        
                                    <th>
                                    </th>        
                                    <th>
                                    </th>        
                                    <th>
                                        <input type="text" class="form-control" placeholder="Reason" v-model="queryEntity.reason" />
                                    </th>        
                                    <th>
                                    </th>        
                                    <th>
                                    <select id="searchStorageName" style="width: 100%" v-model="queryEntity.storageId">
                                        <option value="">请选择...</option>
                                        @{
                                            foreach (var item in Model)
                                            {
                                                <option value="@item.Value">@item.Key</option>
                                            }
                                        }
                                    </select>
                                    </th>        
                                    <th><search-command></search-command></th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr v-for="item in list">
                                    <td>{{item.productName}}</td>
                                    <td>{{item.stockAmount}}</td>
                                    <td>{{item.amount}}</td>
                                    <td>{{item.changedAmount}}</td>
                                    <td>{{item.reason}}</td>
                                    <td>{{item.price}}</td>
                                    <td>{{item.storageName}}</td>
                                    <td>
                                        <row-command :id="item.id" title="编辑" :name="item.name"></row-command>
                                    </td>
                                </tr>
                            </tbody>
                            <tfoot is="table-foot" colspan="8"></tfoot>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<dialog-delete></dialog-delete>
<form-edit></form-edit>
<template id="checkFormBody">
    <div id="form">
        <validator name="v">
            <form class="form-horizontal " name="form" novalidate>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">ProductId<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <select id="searchEditProductId" style="width: 100%" v-model="item.productId" name="productid" v-validate:productid="{required:true}">
                                    <option value="">请选择...</option>
                                    @{
                                        foreach (var item in Model)
                                        {
                                            <option value="@item.Value">@item.Key</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                    </div>
                </div> 
               <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">StockAmount<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <div class="spinbox spinbox-horizontal spinbox-right" id="spinboxStockAmount">
                                    <div class="spinbox-buttons	btn-group">
                                        <button type="button" class="btn spinbox-down blueberry">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                        <button type="button" class="btn spinbox-up purple">
                                            <i class="fa fa-plus"></i>
                                        </button>
                                    </div>
                                    <input type="text" class="form-control spinbox-input " placeholder="必填" id="stockAmount" name="stockamount" v-model="item.stockAmount" v-validate:stockamount="{int:true}"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
               <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">Amount<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <div class="spinbox spinbox-horizontal spinbox-right" id="spinboxAmount">
                                    <div class="spinbox-buttons	btn-group">
                                        <button type="button" class="btn spinbox-down blueberry">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                        <button type="button" class="btn spinbox-up purple">
                                            <i class="fa fa-plus"></i>
                                        </button>
                                    </div>
                                    <input type="text" class="form-control spinbox-input " placeholder="必填" id="amount" name="amount" v-model="item.amount" v-validate:amount="{int:true}"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
               <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">ChangedAmount<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <div class="spinbox spinbox-horizontal spinbox-right" id="spinboxChangedAmount">
                                    <div class="spinbox-buttons	btn-group">
                                        <button type="button" class="btn spinbox-down blueberry">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                        <button type="button" class="btn spinbox-up purple">
                                            <i class="fa fa-plus"></i>
                                        </button>
                                    </div>
                                    <input type="text" class="form-control spinbox-input " placeholder="必填" id="changedAmount" name="changedamount" v-model="item.changedAmount" v-validate:changedamount="{int:true}"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">Reason<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <span class="input-icon icon-right">
                                    <input type="text" class="form-control " placeholder="必填" name="reason" v-model="item.reason"  v-validate:reason="{required:true}"/>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
               <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">Price<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <div class="spinbox spinbox-horizontal spinbox-right" id="spinboxPrice">
                                    <div class="spinbox-buttons	btn-group">
                                        <button type="button" class="btn spinbox-down blueberry">
                                            <i class="fa fa-minus"></i>
                                        </button>
                                        <button type="button" class="btn spinbox-up purple">
                                            <i class="fa fa-plus"></i>
                                        </button>
                                    </div>
                                    <input type="text" class="form-control spinbox-input " placeholder="必填" id="price" name="price" v-model="item.price" v-validate:price="{int:true}"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <div class="form-group" >
                            <label class="col-sm-2 control-label no-padding-right">StorageId<span class="red" >*</span></label>
                            <div class="col-sm-10">
                                <select id="searchEditStorageId" style="width: 100%" v-model="item.storageId" name="storageid" v-validate:storageid="{required:true}">
                                    <option value="">请选择...</option>
                                    @{
                                        foreach (var item in Model)
                                        {
                                            <option value="@item.Value">@item.Key</option>
                                        }
                                    }
                                </select>
                            </div>
                        </div>
                    </div>
                </div> 
            </form>
        </validator>
    </div>
</template>
<script src="~/Scripts/check.js"></script>



