<div class="row">
    <div class="col-xs-12 col-md-12">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">年中调整支出</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                    <input type="file" name="fileUpload" id="fileUpload" class="fileUpload" title="上传年中调整支出" />
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-xs-4 col-md-4">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-danger">
                <span class="widget-caption">年初预算收入表</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar" >
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                                <tr role="row">
                                    <th style="width: 60px;" rowspan="3">
                                        操作
                                    </th>
                                    <th style="width: 200px;" rowspan="3">
                                        科目代码
                                    </th>
                                    <th style="width: 200px;" rowspan="3">
                                        科目名称
                                    </th>
                                    <th style="width: 200px;" rowspan="3">
                                        合计(万元)
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr v-for="item2 in list2">
                                    <td>
                                        <div class="checkbox">
                                            <label for="selectItem_{{item2.key}}">
                                                <input type="radio" v-model="selectItem" value="{{item2.key}}" name="selectItem" id="selectItem_{{item2.key}}">
                                                <span class=" text"></span>
                                            </label>
                                        </div>
                                    </td>
                                    <td>{{item2.value}}</td>
                                    <td>{{item2.item3}}</td>
                                    <td>{{item2.item4}}</td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>    
    <div class="col-xs-8 col-md-8">
        <div class="widget">
            <div class="widget-header bordered-bottom bordered-primary">
                <span class="widget-caption">年初预算支出表</span>
            </div>
            <div class="widget-body">
                <div class="table-toolbar">
                    <div class="row">
                        <div class="col-md-12">
                            <select id="searchSheet" style="width: 300px"></select>
           
                            <button type="button" class="btn btn-primary" v-on:click="update()" v-show="!editModel" ><i class='fa    fa-floppy-o'></i>提交 </button>
                        </div>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <table class="table table-striped table-hover table-bordered ">
                            <thead>
                            <tr role="row">
                                <th style="width: 60px;" rowspan="3">
                                    操作
                                </th>
                                <th style="width: 200px;" rowspan="3">
                                    项目名称(品名)
                                </th>
                                <th style="width: 200px;" rowspan="3">
                                    计量单位
                                </th>
                                <th style="width: 200px;" rowspan="3">
                                    数量
                                </th>
                                <th style="width: 200px;" rowspan="3">
                                    单价或经费标准或支出限额(元)
                                </th>
                                <th style="width: 1000px;" colspan="5">
                                    经费数额
                                </th>
                            </tr>
                            <tr>
                                <th style="width: 200px;" rowspan="2">
                                    合计
                                </th>
                                <th style="width: 600px;" colspan="3">
                                    对下供应
                                </th>

                                <th style="width: 200px;" rowspan="2" rowspan="2">
                                    直接支出
                                </th>
                            </tr>
                            <tr>
                                <th style="width: 200px;">
                                    小计
                                </th>
                                <th style="width: 200px;">
                                    标准经费
                                </th>
                                <th style="width: 200px;">
                                    项目经费
                                </th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr v-for="item in list">
                                <td>
                                    <div class="checkbox">
                                        <label for="selectItems_{{item.id}}">
                                            <input type="checkbox" v-model="selectItems" value="{{item.id}}" name="selectItems" id="selectItems_{{item.id}}">
                                            <span class=" text"></span>
                                        </label>
                                    </div>
                                </td>
                                <td>{{item.name}}</td>
                                <td>{{item.unit}}</td>
                                <td>{{item.amount}}</td>
                                <td>{{item.price}}</td>
                                <td>{{item.total1}}</td>
                                <td>{{item.total2}}</td>
                                <td>{{item.column1}}</td>
                                <td>{{item.column2}}</td>
                                <td>{{item.column3}}</td>

                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<script src="~/assets/js/liteuploader.js"></script>
<script src="~/Views/BudgetOutlay/budgetOutlay2.js"></script>







